var s=[1,2,4,-7,-6];
var m=s.length;
var a=sumarray(s,m);
console..log(a);
function sumarray(a,n)
{
let maxsum;
let cursum;
maxsum=cursum=0;

for(i=0;i<n;i++)
{
    cursum +=a[i];
   if(cursum<a[i])
   {
       cursum=a[i];
   }
   if(maxsum<cursum)
   {
       maxsum=cursum;
   }
    
}
return maxsum;
}

