var s=[-1,-2,-4,-7,6];
var m=s.length;
subarray(s,m);
function subarray(a,f)
{
for(i=0;i<f;i++)
{   
    let y=[];
    for(j=i;j<f;j++)
    {
        y.push(a[j]);
        console.log(y);
    }
}
}

OUTPUT:
[ -1 ]
[ -1, -2 ]
[ -1, -2, -4 ]
[ -1, -2, -4, -7 ]
[ -1, -2, -4, -7, 6 ]
[ -2 ]
[ -2, -4 ]
[ -2, -4, -7 ]
[ -2, -4, -7, 6 ]
[ -4 ]
[ -4, -7 ]
[ -4, -7, 6 ]
[ -7 ]
[ -7, 6 ]
[ 6 ]
