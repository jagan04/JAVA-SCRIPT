var s=[-1,-2,-4,-7,6];
var m=s.length;
for(i=0;i<m;i++)
{   
    let y=[];
    for(j=i;j<m;j++)
    {
        y.push(s[j]);
        console.log(y);
    }
}

OUTPUT:
[ -1 ]
[ -1, -2 ]
[ -1, -2, -4 ]
[ -1, -2, -4, -7 ]
[ -1, -2, -4, -7, 6 ]
[ -2 ]
[ -2, -4 ]
[ -2, -4, -7 ]
[ -2, -4, -7, 6 ]
[ -4 ]
[ -4, -7 ]
[ -4, -7, 6 ]
[ -7 ]
[ -7, 6 ]
[ 6 ]
